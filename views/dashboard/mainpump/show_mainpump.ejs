                    <script>
                    $("#button-mainpump-field8").on("click", function(event) {
                        //event.preventDefault();
                        var $this = $(this);
                        BootstrapDialog.show({
                          message: 'ก่อนเปิดปิดปั๊มน้ำหลัก ควรตรวจสอบว่าได้เปิดวาล์วน้ำบางส่วนไว้ ทั้งนี้เพื่อมิให้มีการสูญเสียแรงดันในระบบ ให้ดำเนินการ<% if(mainpump.field8 == '-1' || mainpump.field8 == null || mainpump.field8 == '') {%>เปิด<% } else{ %>ปิด<% } %>การใช้งานปั๊มน้ำหลัก #8?',
                          type: BootstrapDialog.TYPE_WARNING,
                          buttons: [{
                              label: 'ยกเลิก',
                              action: function(dialogRef){
                                  dialogRef.close();
                                    }
                                  },
                            {
                            icon: 'glyphicon glyphicon-check',
                            label: 'ตกลง',
                            cssClass: 'btn-warning',
                            autospin: true,
                            action: function(dialogRef){
                                $this.toggleClass('active');
                                dialogRef.enableButtons(false);
                                dialogRef.setClosable(false);
                                dialogRef.getModalBody().html('กำลังทำการ<% if(mainpump.field8 == '-1' || mainpump.field8 == null || mainpump.field8 == '') {%>เปิด<% } else{ %>ปิด<% } %>การใช้งานปั๊มน้ำหลัก #7...');
                                var post_url;
                                  <% if(mainpump.field8 == '-1' || mainpump.field8 == null || mainpump.field8 == '') { %>
                                    post_url = '/api/mainpump/turnon/<%= mainpump.id %>/<%= mainpump.write_api_key %>/8';
                                  <% } else if(mainpump.field8 == "1"){ %>
                                    post_url = '/api/mainpump/turnoff/<%= mainpump.id %>/<%= mainpump.write_api_key %>/8';
                                  <% } %>
                                  //console.log("post url => ", post_url);
                                  $.post(post_url, {},
                                    function( response ) {
                                      //console.log("response => ", response.mainpump.field4);
                                      if(response == "200")
                                      {
                                        dialogRef.close();
                                        window.location.reload();
                                      }
                                      else{
                                        <% if(mainpump.field8 == '-1' || mainpump.field8 == null || mainpump.field8 == '') { %>
                                          $.gritter.add({
                                            title:	'ผิดพลาด',
                                            text:	'เปิดปั๊มน้ำหลัก #8 ไม่สำเร็จ',
                                            sticky: true
                                          });
                                        <% } else if(mainpump.field8 == "1"){ %>
                                          $.gritter.add({
                                            title:	'ผิดพลาด',
                                            text:	'ปิดปั๊มน้ำหลัก #8 ไม่สำเร็จ',
                                            sticky: true
                                          });
                                        <% } %>
                                        $this.toggleClass('active');
                                        dialogRef.close();
                                      }
                                  });

                                //console.log("Post url => ", post_url);
                            }
                        }]
                        });
                    });
                  </script>
