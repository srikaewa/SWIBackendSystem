<script>
Highcharts.chart('container_kc', {
    chart: {
        type: 'column'
    },
    title: {
        text: 'Kc'
    },
    subtitle: {
        text: 'Source: Suranaree University of Technology'
    },
    xAxis: {
        categories: [
            <% for(i=1;i<=plant.weeks;i++) { %>
              <%= i %>,
            <% } %>
        ],
        crosshair: true
    },
    yAxis: {
        min: 0,
        title: {
            text: 'Kc'
        }
    },
    tooltip: {
        headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
        pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
            '<td style="padding:0"><b>{point.y:.1f} mm</b></td></tr>',
        footerFormat: '</table>',
        shared: true,
        useHTML: true
    },
    plotOptions: {
        column: {
            pointPadding: 0.2,
            borderWidth: 0
        }
    },
    series: [{
        name: '<%= plant.title %>',
        data: [<% for(var i=0;i<plant.weeks;i++) { %>
          <%= plant.kc[i] %>,
          <% } %>
        ]
    }]
});
</script>
